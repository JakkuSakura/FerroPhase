hir::Program {
    pub struct Foo {
        pub a: i64,
        pub common: i64,
        pub foo: i64,
    }

    pub struct Bar {
        pub common: i64,
        pub bar: i64,
    }

    pub struct InlineRecord {
        pub tag: i64,
        pub value: i64,
    }

    pub fn describe_union(value: struct { a: i64, common: i64, foo: i64 } | struct { common: i64, bar: i64 }) -> i64 {
        // params: [value: struct { a: i64, common: i64, foo: i64 } | struct { common: i64, bar: i64 }]
        {
            match (value)
            {
                Foo { a: a, common: common, foo: foo } =>
                    ((a + common) + foo)
                Bar { common: common, bar: bar } =>
                    (common + bar)
            }
        }
    }

    pub fn print_display<T>(value: T) -> () {
        // params: [value: T]
        {
            std::Println(1 args);
        }
    }

    pub fn describe_optional(value: struct { a: i64, common: i64, foo: i64 } | null) -> i64 {
        // params: [value: struct { a: i64, common: i64, foo: i64 } | null]
        {
            match (value)
            {
                Foo { a: a, common: common, foo: foo } =>
                    ((a + common) + foo)
                null =>
                    0
            }
        }
    }

