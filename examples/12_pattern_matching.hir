            printf("classify(4) = %s\n", classify(4));
            printf("classify(7) = %s\n", classify(7));
            printf("unwrap_or(Some(42), 0) = %lld\n", unwrap_or(Option::Some(42), 0));
            printf("unwrap_or(None, 99) = %lld\n", unwrap_or(Option::None, 99));
