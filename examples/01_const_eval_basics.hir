hir::Program {
    pub fn main() -> () {
        {
            pub const BUFFER_SIZE: i64 =
                4096;
            pub const MAX_CONNECTIONS: i64 =
                150;
            pub const FACTORIAL_5: i64 =
                120;
            pub const IS_LARGE: bool =
                true;
            printf("Buffer: %lldKB, factorial(5)=%lld, large=%d
", (BUFFER_SIZE / 1024), FACTORIAL_5, IS_LARGE);
            pub struct Config {
                pub buffer_size: i64,
                pub max_connections: i64,
            }
            pub const DEFAULT_CONFIG: Config =
                Config { buffer_size: 4096, max_connections: 150 };
            printf("Config: %lldKB buffer, %lld connections
", (DEFAULT_CONFIG.buffer_size / 1024), DEFAULT_CONFIG.max_connections);
            let runtime_multiplier = 3;
            let optimized_size = 8192;
            let cache_strategy = "large";
            let total_memory = (runtime_multiplier * std::ConstBlock(1 args));
            printf("Const blocks: size=%lld, strategy=%s, memory=%lld
", optimized_size, cache_strategy, total_memory);
        }
    }
}
