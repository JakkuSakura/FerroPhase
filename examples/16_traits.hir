hir::Program {
    pub const Shape: bool =
        false;

    pub struct Circle {
        pub radius: f64,
    }

    impl Shape for Circle {
        fn area(self: &Circle) -> f64 {
            // params: [self: &Circle]
            {
                ((3.14159 * self.radius) * self.radius)
            }
        }

        fn describe(self: &Circle) -> () {
            // params: [self: &Circle]
            {
                std::Println(1 args);
            }
        }
    }

    pub struct Rectangle {
        pub width: f64,
        pub height: f64,
    }

    impl Shape for Rectangle {
        fn area(self: &Rectangle) -> f64 {
            // params: [self: &Rectangle]
            {
                (self.width * self.height)
            }
        }

        fn describe(self: &Rectangle) -> () {
            // params: [self: &Rectangle]
            {
                std::Println(1 args);
            }
        }
    }

    pub fn print_area<T>(shape: &T) -> () {
        // params: [shape: &T]
        {
            printf("%.2f\n", shape.area());
        }
    }

    pub fn main() -> () {
        {
            printf("ðŸ“˜ Tutorial: 16_traits.fp\n");
            printf("ðŸ§­ Focus: Traits: defining shared behavior with default methods\n");
            printf("ðŸ§ª What to look for: labeled outputs below\n");
            printf("âœ… Expectation: outputs match labels\n");
            printf("\n");
            let circle = Circle { radius: 5 };
            let rect = Rectangle { width: 4, height: 6 };
            circle.describe();
            rect.describe();
            print_area(circle);
            print_area(rect);
        }
    }
}
