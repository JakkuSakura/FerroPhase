hir::Program {
    fn main() -> () {
        {
            struct Point {
                pub x: f64,
                pub y: f64,
            }
            struct Color {
                pub r: u8,
                pub g: u8,
                pub b: u8,
            }
            const POINT_SIZE: u64 =
                16;
            const COLOR_SIZE: u64 =
                24;
            const POINT_FIELDS: u64 =
                2;
            const COLOR_FIELDS: u64 =
                3;
            const POINT_HAS_X: bool =
                true;
            const POINT_HAS_Z: bool =
                false;
            const POINT_METHODS: u64 =
                0;
            const COLOR_METHODS: u64 =
                0;
            printf("=== Struct Introspection ===
");
            printf("Point size: %d bytes
", std::SizeOf(1 args));
            printf("Color size: %d bytes
", std::SizeOf(1 args));
            printf("Point fields: %d
", std::FieldCount(1 args));
            printf("Color fields: %d
", std::FieldCount(1 args));
            printf("Point has x: %d
", std::HasField(2 args));
            printf("Point has z: %d
", std::HasField(2 args));
            printf("Point methods: %d
", std::MethodCount(1 args));
            printf("Color methods: %d
", std::MethodCount(1 args));
            printf("
✓ Introspection completed!
");
            printf("
=== Transpilation Demo ===
");
            const POINT_SIZE_CONST: u64 =
                16;
            const COLOR_SIZE_CONST: u64 =
                24;
            const TOTAL_SIZE: u64 =
                40;
            let origin = Point { x: 0, y: 0 };
            let red = Color { r: 255, g: 0, b: 0 };
            printf("Transpilation target sizes:
");
            printf("  Point: %d bytes (const)
", POINT_SIZE_CONST);
            printf("  Color: %d bytes (const)
", COLOR_SIZE_CONST);
            printf("  Combined: %d bytes
", TOTAL_SIZE);
            printf("Runtime instances:
");
            printf("  Origin: (%f, %f)
", origin.x, origin.y);
            printf("  Red: rgb(%d, %d, %d)
", red.r, red.g, red.b);
            printf("
✓ Introspection enables external code generation!
");
        }
    }
}
