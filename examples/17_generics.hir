hir::Program {
    pub struct Pair<T, U> {
        pub first: T,
        pub second: U,
    }

    impl Pair<T, U> {
        fn new(first: T, second: U) -> Self {
            // params: [first: T, second: U]
            {
                Self { first: first, second: second }
            }
        }
    }

    pub enum Option<T> {
        Some,
        None,
    }

    impl Option<T> {
        fn unwrap_or(self: Option<T>, default: T) -> T {
            // params: [self: Option<T>, default: T]
            {
                match (self)
                {
                    Option::Some(v) =>
                        v
                    Option::None =>
                        default
                }
            }
        }
    }

    pub fn max<T>(a: T, b: T) -> T {
        // params: [a: T, b: T]
        {
            if ((a > b))
                {
                    a
                }
            else
                {
                    b
                }
        }
    }

    pub fn main() -> () {
        {
            let pair = Pair::new(42, "hello");
            std::Println(2 args);
            std::Println(1 args);
            std::Println(1 args);
            let some: Option<i64> = Option::Some(100);
            let none: Option<i64> = Option::None;
            std::Println(1 args);
            std::Println(1 args);
        }
    }
}
